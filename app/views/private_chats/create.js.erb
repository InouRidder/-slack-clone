Chat.register(<%= @chat_room.id %>, <%= current_user.id %>)
Chat.setChatRoom("<%= j render 'chat_rooms/chat_room', chat_room: @chat_room, message: @message %>");
if (Notifyer.watchList[<%= @chat_room.id %>] === undefined) {
  Chat.appendChat( "<%= j render 'chat_rooms/chat_room_links', chat_room: @chat_room %>", true);
}
Chat.removeModal();
